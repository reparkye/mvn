<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="test.mvn.sp.TestMapper">
  <select id="selectTest" resultType="test">
    select * from test
    <where>
    	<if test="tnum!=null">
    		tnum = #{tnum}
    	</if>
    </where>
  </select>
</mapper> 